@model IEnumerable<Wisata.DataAccess.Models.kecamatan>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Daftar Kecamatan</h2>

<p>
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-success" })
</p>
<table class="table">
    <tr>
        <th>
          
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nama_Kecamatan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.HiddenFor(modelItem => item.Id_Kecamatan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nama_Kecamatan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id_Kecamatan }, new { @class = "btn btn-primary" }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id_Kecamatan }, new { @class = "btn btn-warning" }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id_Kecamatan }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
