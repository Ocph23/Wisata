@model IEnumerable<Wisata.DataAccess.Models.objek>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Daftar Objek Wisata</h2>

<p>
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-success" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KecamatanName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nama_Objek)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Jenis)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lintang)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bujur)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KecamatanName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nama_Objek)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Jenis)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Lintang)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bujur)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ObjekID } , new { @class = "btn btn-primary" }) |
            @Html.ActionLink("Details", "Details", new { id=item.ObjekID }, new { @class = "btn btn-warning" }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ObjekID },new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
