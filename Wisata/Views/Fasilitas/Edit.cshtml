@model Wisata.DataAccess.Models.Fasilitas

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Edit Fasilitas</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <h4>Fasilitas</h4>
        <hr />
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.FasilitasID)

        <div class="form-group">
            @Html.LabelFor(model => model.ObjectId, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.ObjectId)
                @Html.ValidationMessageFor(model => model.ObjectId)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.Nama_Fasilitas, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.Nama_Fasilitas)
                @Html.ValidationMessageFor(model => model.Nama_Fasilitas)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-success" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
