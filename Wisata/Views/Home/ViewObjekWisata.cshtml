@model IEnumerable<Wisata.Models.ObjectWisataView>

@{
    ViewBag.Title = "Home Page";
}




<div class="page-header">
    <div class="row" style="align-content:center">
        <h1>Wisata Jayapura</h1>
        <img src="~/Images/Etsy%20Error_Cropped.jpg" style=" width:100%; height:400px" />
    </div>
</div>
<div class="row">
    <h1>Wisata Jayapura</h1>
    <hr />
    @foreach (Wisata.Models.ObjectWisataView i in Model)
    {

            <div class="col-lg-4">
                <div class="panel panel-warning">
                    <div class="panel-heading">
                        @Html.ActionLink(i.ObjectWisata.Nama_Objek, "Index", "DetailWisata",new { id = i.ObjectWisata.ObjekID },null)
                        
                    </div>

               
                </div>
            </div>
        
    }
</div>
