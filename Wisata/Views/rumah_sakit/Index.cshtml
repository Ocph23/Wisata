@model IEnumerable<Wisata.DataAccess.Models.rumah_sakit>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Tambah Rumah Sakit</h2>

<p>
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-success" })
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.KecamatanName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nama_Rumah_sakit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Alamat)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.KecamatanName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nama_Rumah_sakit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Alamat)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Rumah_SakitID }, new { @class = "btn btn-primary" }) |
            @Html.ActionLink("Details", "Details", new { id = item.Rumah_SakitID }, new { @class = "btn btn-warning" }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Rumah_SakitID }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
