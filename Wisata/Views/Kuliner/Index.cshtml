@model IEnumerable<Wisata.DataAccess.Models.kuliner>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Daftar Rumah Makan Dan Restaurant</h2>

<p>
    @Html.ActionLink("Create New", "Create", null,new { @class = "btn btn-success" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KecamatanName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nama_Tempat_Kuliner)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telpon)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pemilik)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.KecamatanName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nama_Tempat_Kuliner)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telpon)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pemilik)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.KulinerID }, new { @class = "btn btn-primary" }) |
            @Html.ActionLink("Details", "Details", new { id = item.KulinerID }, new { @class = "btn btn-warning" }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.KulinerID }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
